<?xml version="1.0" encoding="UTF-8"?>
<assembly xmlns="http://maven.apache.org/ASSEMBLY/2.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.0.0 http://maven.apache.org/xsd/assembly-2.0.0.xsd">
  <id>zip-assembly</id>
  <formats>
    <format>zip</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <fileSets>
    <fileSet>
      <directory>FsSonarRunner/bin/${configuration}/net472</directory>
      <outputDirectory>\</outputDirectory>
      <includes>
        <include>*.dll</include>
        <include>*.exe</include>
        <include>*.config</include>
        <include>*/FSharp.Core.resources.dll</include>
      </includes>
    </fileSet>
  </fileSets>
</assembly>
